@model OneTraineResultViewModel
<div>
    <h2>@Model.Name</h2>
    @if (Model.Courses!=null) { 
        <ul>
        @for (int i = 0; i < Model.Courses.Count; i++)
        {
            <li>@Model.Courses[i]  Degree is  @Model.Result[i] </li>
        }
        </ul>
    }
    else
    {
        <p>there is no Degree</p>   
    }
</div>